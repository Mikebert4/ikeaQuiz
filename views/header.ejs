<!-- Style -->
<style type="text/css">
/* Apply some basic styling to the body */
body {
  font-family: Arial, sans-serif;
  background-color: #f7f7f7;
  margin: 0;
  padding: 0;
}

/* Style the navigation bar */
.navbar {
  background-color: #343a40;
  color: #fff;
}

/* Center the navbar brand */
.navbar-brand {
  display: flex;
  align-items: center;
}

/* Style the card */
.card {
  border: none;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

/* Style the card header */
.card-header {
  background-color: #343a40;
  color: #fff;
  font-weight: bold;
}

/* Style the hero element */
.hero-element {
  background-color: #007bff;
  color: #fff;
  padding: 20px;
  border-radius: 5px;
  margin-bottom: 20px;
}

/* Style the clickable divs */
.clickable-div {
  background-color: #fff;
  border: 1px solid #ccc;
  border-radius: 5px;
  padding: 15px;
  margin-bottom: 20px;
  cursor: pointer;
  transition: background-color 0.3s ease;
}

.clickable-div:hover {
  background-color: #f0f0f0;
}

/* Center align the page content */
.container {
  text-align: center;
  margin-top: 20px;
}
</style>

</head>
